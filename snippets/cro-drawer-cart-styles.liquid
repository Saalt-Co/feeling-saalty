        display: flex;
        justify-content: center;
        color: #535353;
    }
    [data-slt010-class="slt010-countdown-container"]{
        height: auto;
        margin: 0 0 10px;
        padding: 15px;
        background-color: #f7d6c5;
        border-radius: 5px;
        display: flex;
        flex-direction: column;
    }
    [data-slt010-class="slt010-countdown-title"]{
        width: 100%;
        height: auto;
        margin: 0;
        padding: 0;
        display: flex;
        flex: 0 0 auto;
        justify-content: center;
        align-items: center;
        color: #535353;
        text-transform: uppercase;
        font-size: 18.31px !important;
        letter-spacing: 1.14px;
    }
    [data-slt010-class="slt010-countdown-content"]{
        width: 100%;
        height: auto;
        margin: 0;
        padding: 0;
        display: flex;
        flex: 0 0 auto;
        justify-content: center;
        align-items: center;
    }
    [data-slt010-class="slt010-countdown-msg"]{
        margin-right: 3px;
        display: flex;
        flex: 0 0 auto;
        justify-content: center;
        align-items: center;
        color: #535353;
    }
    [data-slt010-class="slt010-countdown"]{
        display: flex;
        flex: 0 0 auto;
        justify-content: center;
        align-items: center;
    }
    [data-countdown-wrap="slt010-countdown"],
    [data-countdown-wrap="slt010-countdown-mobile"]{
        display: flex;
        flex: 0 0 auto;
        justify-content: center;
        align-items: center;
        color: #535353;
    }
    [data-countdown-wrap="slt010-countdown"] > div,
    [data-countdown-wrap="slt010-countdown-mobile"] > div{
        display: flex;
        flex: 0 0 auto;
    }
    
    [data-slt010-class="slt010-cart-page-countdown"]{
        width: 100%;
        height: auto;
        margin: 0;
        padding: 0;
        display: flex;
        justify-content: center;
    }
    [data-slt010-class="slt010-cart-page-countdown"] [data-slt010-class="slt010-countdown-container"]{
        flex: 0 0 100%;
        width: 100%;
        max-width: 1000px;
    }
    
    [data-shipping-progress-bar-inner] {
        width: 100%;
        height: auto;
        margin: 0;
        padding: 10px;
        background-color: var(--data-shipping-progress-bar-bg-color);
        border: solid 1px var(--data-shipping-progress-bar-border-color);
        display: flex;
    }
    [data-shipping-progress-bar-wrap] {
        width: 100%;
        height: auto;
        margin: 0;
        padding: 0;
        display: flex;
        flex: 0 0 100%;
        flex-wrap: wrap;
        justify-content: center;
        align-items: center;
    }
    [data-shipping-progress-bar-txt-wrap]{
        width: auto;
        max-width: 100%;
        height: auto;
        margin: 0;
        padding: 0 5px 0 0;
        display: flex;
        flex: 0 0 auto;
        justify-content: center;
        align-items: center;
    }
    [data-shipping-progress-bar-msg]{
        color: var(--data-shipping-progress-bar-font-color);
        text-align: center;
        font-size: var(--data-shipping-progress-bar-font-size);
    }
    
    [data-shipping-progress-bar-bar-wrap]{
        width: 100%;
        height: 10px;
        margin: 5px 10px;
        padding: 0;
        background-color: var(--data-shipping-progress-bar-progress-bar-bg);
        border-radius: 10px;
        display: flex;
        flex: 0 0 100%;
        justify-content: flex-start;
        align-items: center;
        overflow: hidden;
    }
    [data-shipping-progress-bar-bar]{
        width: 0;
        height: 10px;
        margin: 0;
        padding: 0;
        background-color: var(--data-shipping-progress-bar-progress-bar);
        border-radius: 10px;
        display: flex !important;
        transition-delay: 100ms;
        transition-duration: 1000ms;
        transition-property: width;
        transition-timing-function: ease;
    }
    [data-shipping-progress-bar-bar].data-shipping-progress-bar-bar-complete{
        background-color: var(--data-shipping-progress-bar-progress-bar-complete);
    }
    
    
    @media all and (max-width: 767px) {  
        [data-shipping-progress-bar-bar-wrap]{
            margin-top: 8px;
        }
    }
    body.slt010-cart-drawer-block-open{
        width: 100vw;
        height: 100vh;
        overflow: hidden;
    }
    #shopify-section-{{ section.id }}{
        width: 0;
        height: 100vh;
        margin: 0;
        padding: 0;
        display: block;
        position: fixed;
        top:0;
        left: 100%;
        z-index: 99;
    }

    slt010-cart-drawer-block{
        width: 0;
        height: 100%;
        margin: 0 auto;
        padding: 0;
        display: none;
        position: fixed;
        top: 0;
        left: 100%;
        opacity: 0;
        overflow: hidden;
        z-index: -9999;
        transition-delay: 1ms;
        transition-duration: 20ms;
        transition-property: all;
        transition-timing-function: ease;
    }
    slt010-cart-drawer-block[data-opened="true"] {
        width: 100vw;
        display: flex;
        left: 0;
        opacity: 1;
        z-index: 99;
        transition-delay: 1ms;
        transition-duration: 20ms;
        transition-property: all;
        transition-timing-function: ease;
    }

    slt010-cart-drawer-block slt010-cart-drawer-overlay{
        width: 100vw;
        height: 100vh;
        margin: 0;
        padding: 0;
        display: flex !important;
        position: absolute;
        top: 0;
        left: 0;
        background-color: rgba(0,0,0,.5);
        z-index: -1;
        opacity: 0;
        transition-delay: 1ms;
        transition-duration: 1000ms;
        transition-property: all;
        transition-timing-function: ease;
    }
    slt010-cart-drawer-block[data-opened="true"] slt010-cart-drawer-overlay{
        left: 0;
        opacity: 1;
        transition-delay: 1ms;
        transition-duration: 2000ms;
        transition-property: all;
        transition-timing-function: ease;
    }

    slt010-cart-drawer-block slt010-cart-drawer-container{
        width: 90vw;
        max-width: 500px;
        height: 100%;
        margin: 0;
        padding: 0;
        display: flex;
        flex: 0 0 90vw;
        position: absolute;
        top: 0;
        right: -100%;
        overflow: hidden;
        z-index: 0;
        transition-delay: 1ms;
        transition-duration: 900ms;
        transition-property: right;
        transition-timing-function: ease;
    }
    slt010-cart-drawer-block[data-opened="true"] slt010-cart-drawer-container{
        right: 0;
        transition-delay: 1ms;
        transition-duration: 1000ms;
        transition-property: right;
        transition-timing-function: ease;
    }
