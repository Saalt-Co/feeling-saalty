<style id="{{ section.id }}-style">
    :root{
        --data-shipping-progress-bar-bg-color: #fff;
        --data-shipping-progress-bar-progress-bar-bg: #eee;
        --data-shipping-progress-bar-progress-bar: #f7d6c5;
        --data-shipping-progress-bar-progress-bar-complete: #f7d6c5;
        --data-shipping-progress-bar-font-color: #3f3f3f;
        --data-shipping-progress-bar-font-size: 14px;
        --data-shipping-progress-bar-border-color: #e1e1e1;
    }
    
