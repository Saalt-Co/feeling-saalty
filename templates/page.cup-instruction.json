    "b7db5103-2c7b-4c05-a774-90295600e617": {
      "type": "saalt-media-with-text",
      "blocks": {
        "template--21580602507537__b7db5103-2c7b-4c05-a774-90295600e617-section_title-1": {
          "type": "section_title",
