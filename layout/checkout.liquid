      <script>
        const elementReady = (selector) => {
          return new Promise((resolve, reject) => {
            let el = document.querySelector(selector);
            if (el) {resolve(el);}
            new MutationObserver((mutationRecords, observer) => {
              {% comment %}// Query for elements matching the specified selector{% endcomment %}
              Array.from(document.querySelectorAll(selector)).forEach((element) => {
                resolve(element);
                {% comment %}//Once we have resolved we don't need the observer anymore.{% endcomment %}
                observer.disconnect();
              });
            })
            .observe(document.documentElement, {
              childList: true,
              subtree: true
            });
          });
        }
  
        document.addEventListener('DOMContentLoaded', function () {
          elementReady("#terms-wrapper").then(() => {
            const termsWrapper = document.querySelector("#terms-wrapper");
            const mainHeaderNav = document.querySelector(".main__header").querySelector("nav");
            mainHeaderNav.insertAdjacentElement("afterend", termsWrapper);
          });
        }, false);

        const $mobileBanner = document.querySelector('[data-slt009-class="slt009-countdown-container-mobile"]');
        const $breadcrumb = document.querySelector('nav[aria-label="Breadcrumb"]');
        $breadcrumb.appendChild($mobileBanner);
