    {%- if settings.show-slt011-countdown -%}  
      <script>
        class countdownBlock extends HTMLElement{
            constructor(){
                super();
                this.dates = {
                    current: '',
                    end: ''
                };
            }
            connectedCallback(){
                console.log('slt011-countdown-block added');
                this.create();
            }
        
            each($data, $callback){
                for(let $c = 0; $c < $data.length; $c++){
                    $callback($data[$c], $c);
                }
            }
            setCurrentDate(){
                this.dates.current = new Date();
            }
            setEndDate($data){
                this.dates.end = new Date($data.year, $data.month, $data.day, $data.hours, $data.minutes, $data.seconds);
            }
            getMinutes(){
                const $e = this.dates.end.getTime() - this.dates.current.getTime();
                const $d = Math.floor($e / (24 * 60 * 60 * 1000));
                const $h = Math.floor(($e - $d * (24 * 60 * 60 * 1000)) / (60 * 60 * 1000));
                const $m = Math.floor(($e - $d * (24 * 60 * 60 * 1000) - $h * (60 * 60 * 1000)) / 60000);
                return ($m < 10) ? '0' + $m : $m;
            }
            getSeconds(){
                const $e = this.dates.end.getTime() - this.dates.current.getTime();
                const $d = Math.floor($e / (24 * 60 * 60 * 1000));
                const $h = Math.floor(($e - $d * (24 * 60 * 60 * 1000)) / (60 * 60 * 1000));
                const $m = Math.floor(($e - $d * (24 * 60 * 60 * 1000) - $h * (60 * 60 * 1000)) / 60000);
                const $s = Math.floor(($e - $d * (24 * 60 * 60 * 1000) - $h * (60 * 60 * 1000) - $m * (60000)) / 1000);
                return ($s < 10) ? '0' + $s : $s;
            }
            create(){
                const $st = new Date();
                const $et = new Date($st.getTime() + ({{ settings.slt011-timer }} * 60 * 1000));
                const $endTime = {
                    year: $et.getFullYear(),
                    month: $et.getMonth(),
                    day: $et.getDate(),
                    hours: $et.getHours(),
                    minutes: $et.getMinutes(),
                    seconds: $et.getSeconds()
                };
                const $c = [
                    'minutes',
                    'seconds'
                ];
                const $w = document.createElement('div');
                $w.setAttribute('data-countdown-wrap','wrapper');
                this.each($c, ($a, $b)=>{
                    const $d = document.createElement('div');
                    const $s = document.createElement('span');
                    $s.setAttribute('data-countdown-' + $a, $b);
                    $d.appendChild($s);
                    $w.appendChild($d);
                });
                this.appendChild($w);
        
                this.setCurrentDate();
                this.setEndDate($endTime);
                this.start();
            }
            start(){
                const $t = this;
                $t.update();
                let $timer = setInterval(()=>{
                    $t.setCurrentDate();
                    const $time = $t.update();
                    if(!$time){
                        clearInterval($timer);
                    }
                }, 1000);
            }
            update(){
                let $value = true;
                if(this.dates.end.getTime() > this.dates.current.getTime()){
                    this.querySelector('[data-countdown-minutes]').innerHTML = this.getMinutes();
                    this.querySelector('[data-countdown-seconds]').innerHTML = this.getSeconds();
                }
                else{
                    this.style.display = 'none';
                    this.closest(this.getAttribute('data-wrapper-class')).style.display = 'none';
                    $value = false;
                }
                return $value;
            }
        }
        if(customElements.get('slt011-countdown-block') === undefined){
          customElements.define('slt011-countdown-block', countdownBlock);
        }
      </script>
