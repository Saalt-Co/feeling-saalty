      {%- if settings.show_slt009_msg -%}
        [data-slt009-class="slt009-countdown-container"]{
            width: calc(100% - 50px);
            height: auto;
            margin: 0 0 20px;
            padding: 25px;
            background-color: #f7d6c5;
            border-radius: 5px;
            display: flex;
            flex-direction: column;
        }
        [data-slt009-class="slt009-countdown-container-mobile"]{
            display: none;
        }
        [data-slt009-class="slt009-countdown-title"]{
            width: 100%;
            height: auto;
            margin: 0;
            padding: 0;
            display: flex;
            flex: 0 0 auto;
            justify-content: center;
            align-items: center;
            color: #535353;
            text-transform: uppercase;
            font-size: 18.31px;
            letter-spacing: 1.14px;
        }
        [data-slt009-class="slt009-countdown-content"]{
            width: 100%;
            height: auto;
            margin: 0;
            padding: 0;
            display: flex;
            flex: 0 0 auto;
            justify-content: center;
            align-items: center;
        }
        [data-slt009-class="slt009-countdown-msg"]{
            margin-right: 3px;
            display: flex;
            flex: 0 0 auto;
            justify-content: center;
            align-items: center;
            color: #535353;
        }
        
        @media all and (max-width: 767px) {  
            [data-slt009-class="slt009-countdown-container"]{
                display: none;
            }
            [data-slt009-class="slt009-countdown-container-mobile"]{
                width: calc(100% - 50px);
                height: auto;
                margin: 0;
                padding: 25px;
                background-color: #f7d6c5;
                border-radius: 5px;
                display: flex;
                flex-direction: column;
            }
        }
      {%- endif -%}
      </style>
  
